{
  "version": "1.0.0-*",
  "dependencies": {
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "xunit": "2.1.0",
    "dotnet-test-xunit": "2.2.0-preview2-build1029",
    "WeatherLink": {
      "target": "project"
    }
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.1"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  },
  "buildOptions": {
    "debugType": "portable",
    "copyToOutput": {
			"include": [
			"Data/*"
			]
    }
  },
  "publishOptions": {
    "include": [
      "Dockerfile.debug",
      "Dockerfile",
      "docker-compose.debug.yml",
      "docker-compose.yml"
    ]
  }
}